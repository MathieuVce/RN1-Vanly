{
  "expo": {
    "name": "Vanly",
    "slug": "Vanly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ic.png",
    "splash": {
      "image": "./assets/SplashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.vanly.vanly",
      "permissions" : ["CAMERA", "ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION", "READ_EXTERNAL_STORAGE"],
      "versionCode": 3,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBTmZj2Bq3MYkF5qLnsbV0Q1w5YLuHTVSs"
        }
      }
    },
    "web": {
      "favicon": "./assets/ic.png"
    }
  }
}
